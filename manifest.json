{

    "description": "download file directly to gdrive",
    "manifest_version": 2,
    "name": "downdrive",
    "version": "1.0",
    "homepage_url": "http://127.0.0.1:8000/",
    "icons": {
      "48": "icons/page-48.png"
    },
    "background": {
      "scripts": ["control.js" ]
    },

    "host_permissions": [
      "127.0.0.1:8000"
    ],

    "permissions": [
      "downloads",
      "cookies"
    ],
  
    "browser_action": {
      "browser_style": true,
      "default_icon": "icons/page-32.png",
      "default_title": "Latest download",
      "default_popup": "popup/latest_download.html"
    }
  
  }
  